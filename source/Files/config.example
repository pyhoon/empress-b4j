# Lines starting with '#' are comments.
# Lines starting with ';' are disabled.
# Backslash character at the end of line means that the command continues in the next line.

# App Constants
APP_TITLE=Empress
APP_TRADEMARK=EMPRESS
HOME_TITLE=EMPRESS CMS
APP_COPYRIGHT=Copyright Computerise System Solutions 2025

# Server Path
ROOT_URL=http://127.0.0.1
ROOT_PATH=

# Api Settings
#API_NAME=api

# Server port
PORT=8080
REDIRECT_TO_HTTPS=False

# SSL Settings
# Generate a keystore from Windows CMD
# C:\Java\jdk-19.0.2\bin\keytool -keystore keystore.jks -alias jetty -genkey -keyalg RSA
# Copy keystore file to Objects folder of the B4J project
SSL_PORT=8888
SSL_KEYSTORE_DIR=
SSL_KEYSTORE_FILE=keystore.jks
SSL_KEYSTORE_PASSWORD=xxxxxxxx
SSL_ENABLED=False